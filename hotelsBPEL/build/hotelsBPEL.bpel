<?xml version="1.0" encoding="UTF-8" ?>
<process
    name="hotelsBPEL"
    targetNamespace="http://enterprise.netbeans.org/bpel/hotelsBPEL/hotelsBPEL"
    xmlns:tns="http://enterprise.netbeans.org/bpel/hotelsBPEL/hotelsBPEL"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2"
    xmlns:sxat="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Attachment"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling">
    <import namespace="http://enterprise.netbeans.org/bpel/hotelsWSDLServiceWrapper" location="hotelsWSDLServiceWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://hotels.ws" location="http://localhost:8080/hotelsWS/hotelsWSDLService?wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/hotelsBPEL_WSDLWrapper" location="hotelsBPEL_WSDLWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://hotelsBPEL_WSDL" location="hotelsBPEL_WSDL.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="PartnerLink2" xmlns:tns="http://enterprise.netbeans.org/bpel/hotelsBPEL_WSDLWrapper" partnerLinkType="tns:hotelsBPEL_WSDLLinkType" myRole="hotelsBPEL_WSDLRole"/>
        <partnerLink name="PartnerLink1" xmlns:tns="http://enterprise.netbeans.org/bpel/hotelsWSDLServiceWrapper" partnerLinkType="tns:hotelsWSDLLinkType" partnerRole="hotelsWSDLRole"/>
    </partnerLinks>
    <variables>
        <variable name="GetHotelsOutExternal" xmlns:tns="http://hotels.ws" messageType="tns:getHotelsResponse"/>
        <variable name="GetHotelsInExternal" xmlns:tns="http://hotels.ws" messageType="tns:getHotelsRequest"/>
        <variable name="GetHotelsOut" xmlns:tns="http://hotelsBPEL_WSDL" messageType="tns:getHotelsResponse"/>
        <variable name="GetHotelsIn" xmlns:tns="http://hotelsBPEL_WSDL" messageType="tns:getHotelsRequest"/>
    </variables>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="PartnerLink2" operation="getHotels" xmlns:tns="http://hotelsBPEL_WSDL" portType="tns:hotelsBPEL_WSDLPortType" variable="GetHotelsIn"/>
        <assign name="Assign1">
            <copy>
                <from>$GetHotelsIn.getHotelsRequest/city</from>
                <to>$GetHotelsInExternal.getHotelsRequest/city</to>
            </copy>
            <copy>
                <from>$GetHotelsIn.getHotelsRequest/arrivalDate</from>
                <to>$GetHotelsInExternal.getHotelsRequest/arrivalDate</to>
            </copy>
            <copy>
                <from>$GetHotelsIn.getHotelsRequest/departureDate</from>
                <to>$GetHotelsInExternal.getHotelsRequest/departureDate</to>
            </copy>
        </assign>
        <invoke name="Invoke1" partnerLink="PartnerLink1" operation="getHotels" xmlns:tns="http://hotels.ws" portType="tns:hotelsWSDLPortType" inputVariable="GetHotelsInExternal" outputVariable="GetHotelsOutExternal"/>
        <assign name="Assign2">
            <copy>
                <from>$GetHotelsOutExternal.getHotelsResponse/hotelList/hotel/name</from>
                <to>$GetHotelsOut.getHotelsResponse/hotelList/hotel/name</to>
            </copy>
            <copy>
                <from>$GetHotelsOutExternal.getHotelsResponse/hotelList/hotel/address</from>
                <to>$GetHotelsOut.getHotelsResponse/hotelList/hotel/address</to>
            </copy>
            <copy>
                <from>$GetHotelsOutExternal.getHotelsResponse/hotelList/hotel/bookingNumber</from>
                <to>$GetHotelsOut.getHotelsResponse/hotelList/hotel/bookingNumber</to>
            </copy>
            <copy>
                <from>$GetHotelsOutExternal.getHotelsResponse/hotelList/hotel/totalPrice</from>
                <to>$GetHotelsOut.getHotelsResponse/hotelList/hotel/totalPrice</to>
            </copy>
            <copy>
                <from>$GetHotelsOutExternal.getHotelsResponse/hotelList/hotel/creditcardGuarantee</from>
                <to>$GetHotelsOut.getHotelsResponse/hotelList/hotel/creditcardGuarantee</to>
            </copy>
            <copy>
                <from>$GetHotelsOutExternal.getHotelsResponse/hotelList/hotel/serviceName</from>
                <to>$GetHotelsOut.getHotelsResponse/hotelList/hotel/serviceName</to>
            </copy>
            <copy>
                <from>$GetHotelsOutExternal.getHotelsResponse/hotelList</from>
                <to>$GetHotelsOut.getHotelsResponse/hotelList</to>
            </copy>
        </assign>
        <reply name="Reply1" partnerLink="PartnerLink2" operation="getHotels" xmlns:tns="http://hotelsBPEL_WSDL" portType="tns:hotelsBPEL_WSDLPortType" variable="GetHotelsOut"/>
    </sequence>
</process>
